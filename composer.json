{
    "name": "swooletw/hyperf-router",
    "type": "library",
    "description": "The router package for Hyperf.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "router",
        "swoole",
        "laravel-hyperf"
    ],
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert@laravel-hyperf.com",
            "homepage": "https://albert-chen.com"
        }
    ],
    "support": {
        "issues": "https://github.com/swooletw/hyperf-packages/issues",
        "source": "https://github.com/swooletw/hyperf-packages"
    },
    "autoload": {
        "psr-4": {
            "SwooleTW\\Hyperf\\Router\\": "src/"
        },
        "files": [
            "src/Functions.php"
        ]
    },
    "require": {
        "php": "^8.2",
        "hyperf/context": "~3.1.0",
        "hyperf/http-server": "~3.1.0"
    },
    "suggest": {
        "swooletw/hyperf-cache": "Required to use throttle requests middleware.",
        "swooletw/hyperf-http-message": "Required to use throttle requests middleware.",
        "hyperf/session": "Required to use session based `previous` function in UrlGenerator."
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hyperf": {
            "config": "SwooleTW\\Hyperf\\Router\\ConfigProvider"
        },
        "branch-alias": {
            "dev-main": "3.1-dev"
        }
    }
}
